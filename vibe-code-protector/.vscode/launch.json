{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Extension",
      "program": "${workspaceFolder}/src/extension.js",
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "npm: watch",
      "runtimeArgs": [
        "--inspect-brk"
      ],
      "sourceMaps": true
    }
  ]
}